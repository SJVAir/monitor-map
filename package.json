{
  "name": "@sjvair/monitor-map",
  "description": "SJVAir's interactive monitor map",
  "version": "3.0.0-alpha.1",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run prepack",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "prepack": "svelte-kit sync && svelte-package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint ."
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.4.0",
    "@eslint/js": "^9.36.0",
    "@deno/vite-plugin": "^1.0.5",
    "@maptiler/sdk": "^3.8.0",
    "@sjvair/sdk": "npm:@jsr/sjvair__sdk@^2.0.3",
    "@sveltejs/adapter-auto": "^6.1.0",
    "@sveltejs/adapter-static": "^3.0.9",
    "@sveltejs/enhanced-img": "^0.8.2",
    "@sveltejs/kit": "^2.43.5",
    "@sveltejs/package": "^2.5.4",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tailwindcss/vite": "^4.1.13",
    "@tstk/decorators": "npm:@jsr/tstk__decorators@^1.0.4",
    "@tstk/types": "npm:@jsr/tstk__types@^1.1.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.12.4",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "publint": "^0.3.13",
    "svelte": "^5.39.7",
    "svelte-check": "^4.3.2",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2",
    "uplot": "^1.6.32",
    "vite": "^7.1.7"
  },
  "keywords": [
    "svelte"
  ]
}

{
  "name": "@sjvair/monitor-map",
  "description": "SJVAir's interactive monitor map",
  "files": [
    "dist"
  ],
  "version": "1.36.0",
  "type": "module",
  "scripts": {
    "dev": "VITE_BUILD_MODE='development' vite",
    "dev:host": "VITE_BUILD_MODE='development' vite --host",
    "build": "npm run build:widget && npm run build:module",
    "build:widget": "vue-tsc --noEmit && vite build",
    "build:module": "VITE_BUILD_MODE=mod npm run build:widget",
    "build:pages": "VITE_BUILD_MODE=ghp VITE_REQUEST_MODE=leach npm run build:widget",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@types/d3": "^7.4.0",
    "@types/leaflet": "^1.9.1",
    "@types/leaflet.markercluster": "^1.5.1",
    "@types/node": "18.14.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vuepic/vue-datepicker": "^4.0.1",
    "axios": "^1.3.4",
    "bulma": "^0.9.4",
    "color2k": "^2.0.2",
    "d3": "^7.8.2",
    "dayjs": "^1.11.7",
    "i": "^0.3.7",
    "leaflet": "^1.9.3",
    "leaflet-svg-shape-markers": "^1.4.0",
    "leaflet.markercluster": "^1.5.3",
    "npm": "^9.5.1",
    "sass": "^1.58.3",
    "terser": "^5.16.5",
    "typescript": "^4.9.5",
    "uplot": "^1.6.24",
    "vite": "^4.1.4",
    "vite-plugin-purgecss": "^0.2.12",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6",
    "vue-tsc": "^1.1.7"
  }
}
